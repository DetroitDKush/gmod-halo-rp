"Unlittwotexture"
//"VertexlitGeneric"
{
	"$model" "1"
	"$basetexture" "models/cire992/props/Bio_Cpu_01_Data_ani"
	"$surfaceprop" "glass"
	"$texture2"	"models/cire992/props/fx_holo_distortion"
	"$selfillum" "1"
	"$translucent" 1
	"$mod2x" "1"
//	"$additive" "1"

	"Proxies"
	{
		"AnimatedTexture"
		{
			"Animatedtexturevar" "$basetexture"
			"Animatedtextureframenumvar" "$frame"
			"Animatedtextureframerate" "24"
		}
		"Sine"
		{
			"resultVar" "$color[0]"
			"sineperiod" .2
			"sinemin" .99
			"sinemax" 1
		}
		"Sine"
		{
			"resultVar" "$color[1]"
			"sineperiod" .1
			"sinemin" .98
			"sinemax" 1
		}
		"Sine"
		{
			"resultVar" "$color[2]"
			"sineperiod" .1
			"sinemin" .99
			"sinemax" 1
		}

		"TextureScroll"
		{
			"texturescrollvar" "$texture2transform"
			"texturescrollrate" .08
			"texturescrollangle" -90
		}


	}

}